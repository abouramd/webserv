#Example:
server {
host 127.0.0.1
listen 8083 8082
server_name 1337.com localhost:8082
# max_body_size 10000000


location / {
  index index.php index.html
  root /
  allow_method GET DELET
  auto_index on
  uplode on //
  cgi on
  # cgi_path .php /php-cgi
}
location /test {
 return 42.fr
  root /
}
}

server {
host 127.0.0.1
listen 8080 8081 
server_name 1337.com
max_body_size 10000000


location / {
  index index.php index.html
  root /
  allow_method GET DELET
  auto_index on
  uplode on //
  cgi on
  # cgi_path .php /php-cgi
}
location /test {
 return 42.fr
  root /
}
}





